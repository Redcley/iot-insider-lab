</head>
<body ng-app="app" class="loaded">    
    <preloader></preloader>       
    <!-- Start Page Loading -->
    <div id="loader-wrapper">
        <div id="loader"></div>
        <div class="loader-section section-left"></div>
        <div class="loader-section section-right"></div>
    </div>
    <!-- End Page Loading -->

    @RenderBody()    

    @RenderSection("scripts", required: false)    
